// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Event {
  id         String   @id @default(uuid())
  image      String
  title      String
  status     String
  date       DateTime
  details    String
  location   String
  productors String
  price      Int
}
